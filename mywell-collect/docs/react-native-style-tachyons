aife                   alignItems<span class="pl-k">:</span> <span class="pl-s"><span class="pl-pds">"</span>flex-end<span class="pl-pds">"</span></span>
jcc                    justifyContent<span class="pl-k">:</span> <span class="pl-s"><span class="pl-pds">"</span>center<span class="pl-pds">"</span></span>             <span class="pl-c"><span class="pl-c">/*</span> default: "flex-start" <span class="pl-c">*/</span></span>
jcfe                   justifyContent<span class="pl-k">:</span> <span class="pl-s"><span class="pl-pds">"</span>flex-end<span class="pl-pds">"</span></span>
