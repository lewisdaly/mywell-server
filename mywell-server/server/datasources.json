{
  "mysql-corp": {
    "name": "mysql-corp",
    "host": "${DB_HOST}",
    "port": 3306,
    "database": "${DB_NAME}",
    "password": "${DB_PASSWORD}",
    "user": "${DB_USER}",
    "connector": "mysql"
  },
  "storage": {
    "name": "storage",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "root": "/usr/src/app/storage"
  }
}
